{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT comment.*\n        FROM comments comment\n        JOIN posts post ON comment.post_uuid = post.uuid\n        WHERE comment.author_id = $1\n        AND comment.deleted = false\n        AND post.deleted = false\n        AND post.sold = false\n        ORDER BY comment.updated_at DESC\n        LIMIT $2\n        OFFSET $3\n        ",
